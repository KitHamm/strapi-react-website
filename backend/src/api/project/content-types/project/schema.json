        "website",
        "art",
        "fun"
      ]
    },
    "uid": {
      "type": "uid",
      "targetField": "title"
    },
    "link": {
      "type": "string"
    },
    "short": {
      "type": "text"
    },
    "video_url": {
      "type": "component",
      "repeatable": true,
      "component": "url.video-url"
    },
    "where": {
      "type": "string"
    },
    "tech": {
      "type": "string"
    },
    "order": {
      "type": "integer",
      "unique": true,
      "required": true
    },
    "video": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
